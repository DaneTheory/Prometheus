{
  "name": "prometheus",
  "description": "A starter boilerplate for NPM modules. Includes a few goodies to get you going like ES6, commands for running tests/linting/builds/examples/CI/coverage/publishing, and more.",
  "version": "1.0.0",
  "private": false,
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "istanbul": "cross-env NODE_ENV=development nyc mocha --require babel-core/register --recursive",
    "test": "npm run istanbul",
    "test:watch": "npm run istanbul -- --watch",
    "test:examples": "npm run clean && npm run build && node examples/",
    "lint": "eslint src test",
    "coverage": "cross-env NODE_ENV=development nyc report -- --require babel-core/register --recursive --reporter=coveralls",
    "prod": "cross-env NODE_ENV=development babel src --out-dir lib",
    "build": "npm run prod",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "author": "Branden Dane",
  "homepage": "https://github.com/DaneTheory/Prometheus",
  "contributors": [
    "Branden Dane || Email: <danetheory@gmail.com> Github: (https://github.com/DaneTheory) Website: (http://DaneTheory.com)"
  ],
  "bugs": {
    "url": "https://github.com/DaneTheory/Prometheus/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaneTheory/Prometheus.git"
  },
  "license": "MIT",
  "keywords": [
    "prometheus",
    "danetheory",
    "starter",
    "boilerplate",
    "npm",
    "module",
    "package",
    "node",
    "nodejs",
    "es6",
    "javascript"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "mocha": "^5.0.0",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2"
  }
}
